import sys
#  exponga los meses que superan un cierto umbral.

#gestion de los meses 
vents = {
"Enero": 15000,
"Febrero": 22000,
"Marzo": 12000,
"Abril": 17000,
"Mayo": 81000,
"Junio": 13000,
"Julio": 21000,
"Agosto": 41200,
"Septiembre": 25000,
"Octubre": 21500,
"Noviembre": 91000,
"Diciembre": 21000,
}
#cÃ³digo 
for keys, value in dict.items(vents):
    if value >= int(sys.argv[1]):
        print(f'{keys}: {value}')
